{application, alara,
 [
  {description, "ALARA - Distributed Entropy Network System"},
  {vsn, "0.1.4"},
  {registered, []},
  {applications, [
    kernel,
    stdlib,
    crypto,
    ranch
  ]},
  {env, [
    {default_trust_level, 0.8},
    {consensus_interval, 5000},
    {max_nodes, 1000},
    {entropy_pool_size, 10000}
  ]},
  {modules, [alara]},
  {licenses, ["Apache 2.0"]},
  {links, [{"GitHub", "https://github.com/Green-Mice/alara"}]}
 ]}.

